<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HealthNutritionFragment">

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/todo_desc"
        app:layout_constraintEnd_toEndOf="@+id/nutrition_button1"
        app:layout_constraintStart_toStartOf="@+id/nutrition_button1"
        app:layout_constraintTop_toBottomOf="@+id/nutrition_button1" />

    <Button
        android:id="@+id/nutrition_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:onClick="nutritionButton1Click"
        android:text="@string/nutrition_button1_label"
        app:layout_constraintEnd_toEndOf="@+id/nutrition_button2"
        app:layout_constraintStart_toStartOf="@+id/nutrition_button2"
        app:layout_constraintTop_toBottomOf="@+id/nutrition_button2" />

    <Button
        android:id="@+id/nutrition_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:onClick="nutritionButton2Click"
        android:text="@string/nutrition_button2_label"
        app:layout_constraintEnd_toEndOf="@+id/health_button2"
        app:layout_constraintStart_toStartOf="@+id/health_button2"
        app:layout_constraintTop_toBottomOf="@+id/health_button2" />

    <Button
        android:id="@+id/health_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:onClick="healthButton1Click"
        android:text="@string/health_button1_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/health_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:onClick="healthButton2Click"
        android:text="@string/health_button2_label"
        app:layout_constraintEnd_toEndOf="@+id/health_button1"
        app:layout_constraintStart_toStartOf="@+id/health_button1"
        app:layout_constraintTop_toBottomOf="@+id/health_button1" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/todo_desc"
        app:layout_constraintEnd_toEndOf="@+id/health_button1"
        app:layout_constraintStart_toStartOf="@+id/health_button1"
        app:layout_constraintTop_toBottomOf="@+id/health_button1" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/todo_desc"
        app:layout_constraintEnd_toEndOf="@+id/health_button2"
        app:layout_constraintStart_toStartOf="@+id/health_button2"
        app:layout_constraintTop_toBottomOf="@+id/health_button2" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/todo_desc"
        app:layout_constraintEnd_toEndOf="@+id/nutrition_button2"
        app:layout_constraintStart_toStartOf="@+id/nutrition_button2"
        app:layout_constraintTop_toBottomOf="@+id/nutrition_button2" />

</androidx.constraintlayout.widget.ConstraintLayout>