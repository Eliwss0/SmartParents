<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".K12EducationFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/k12_button5_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/k12education_button5_desc"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button5"
                app:layout_constraintStart_toStartOf="@+id/k12education_button5"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button5" />

            <Button
                android:id="@+id/k12education_button1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:onClick="k12EducationButton1Click"
                android:text="@string/k12education_button1_label"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.485"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/k12education_button2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:onClick="k12EducationButton2Click"
                android:text="@string/k12education_button2_label"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button1"
                app:layout_constraintStart_toStartOf="@+id/k12education_button1"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button1" />

            <Button
                android:id="@+id/k12education_button3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:onClick="k12EducationButton3Click"
                android:text="@string/k12education_button3_label"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button2"
                app:layout_constraintStart_toStartOf="@+id/k12education_button2"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button2" />

            <Button
                android:id="@+id/k12education_button4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:onClick="k12EducationButton4Click"
                android:text="@string/k12education_button4_label"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button3"
                app:layout_constraintStart_toStartOf="@+id/k12education_button3"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button3" />

            <Button
                android:id="@+id/k12education_button5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:onClick="k12EducationButton5Click"
                android:text="@string/k12education_button5_label"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button4"
                app:layout_constraintStart_toStartOf="@+id/k12education_button4"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button4" />

            <TextView
                android:id="@+id/health_button1_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/k12education_button1_desc"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button1"
                app:layout_constraintStart_toStartOf="@+id/k12education_button1"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button1" />

            <TextView
                android:id="@+id/k12_button2_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/k12education_button2_desc"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button2"
                app:layout_constraintStart_toStartOf="@+id/k12education_button2"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button2" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/k12education_button3_desc"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button3"
                app:layout_constraintStart_toStartOf="@+id/k12education_button3"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button3" />

            <TextView
                android:id="@+id/k12_button4_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/k12education_button4_desc"
                app:layout_constraintEnd_toEndOf="@+id/k12education_button4"
                app:layout_constraintStart_toStartOf="@+id/k12education_button4"
                app:layout_constraintTop_toBottomOf="@+id/k12education_button4" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>